{
	"name": "@just-web/types",
	"version": "0.0.0",
	"description": "Common types for web applications",
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./package.json": "./package.json"
	},
	"main": "./src/index.ts",
